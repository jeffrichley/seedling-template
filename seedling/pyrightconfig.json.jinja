{
  "exclude": [
    ".git",
    ".venv",
    ".nox",
    ".mypy_cache",
    ".pytest_cache",
    ".ruff_cache",
    ".hypothesis",
    "__pycache__",
    "htmlcov",
    "tmp",
    "output",
    ".benchmarks",
    "stubs",
    "examples/assets",
    "docs/build",
    "docs/source/_static",
    "vine.zip",
    "monkeytype.sqlite3",
    "test_output.mp4",
    "*.log",
    "*.sqlite3",
    "*.mp4",
    "*.zip"
  ],
  "include": [
    "src",
    "tests",
    "examples",
    "scripts"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": false,
  "reportMissingTypeStubs": false
}
