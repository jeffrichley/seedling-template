"""Performance tests for {{ project_name }}."""

import time

import pytest

from {{ project_slug }} import main_function


@pytest.mark.performance
def test_main_function_execution_time() -> None:
    """Test that main_function completes within acceptable time limit."""
    # Arrange
    input_data = "performance test data"
    max_execution_time = 0.1  # 100ms
    
    # Act
    start_time = time.time()
    result = main_function(input_data)
    execution_time = time.time() - start_time
    
    # Assert
    assert result == "Processed: performance test data"
    assert execution_time < max_execution_time 